<UserControl x:Class="StoryTeller.UserInterface.Dialogs.AddTest"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Padding="5">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
        <StackPanel Orientation="Horizontal">
            <Label>Test Name</Label>
            <TextBox Name="name" Width="300"></TextBox>
        </StackPanel>
        <Button HorizontalAlignment="Left" Name="create" IsDefault="True" Height="25" VerticalContentAlignment="Stretch">
            <StackPanel Orientation="Horizontal">
                <Image Name="icon"></Image>
                <Label Height="25" Padding="0">Create</Label>
            </StackPanel>
            
            
        </Button>
    </StackPanel>
</UserControl>
